{
  "description": "Default base config for AIX",
  "extends": [
    ":pinOnlyDevDependencies",
    ":rebaseStalePrs",
    ":renovatePrefix",
    "schedule:nonOfficeHours",
    ":semanticCommits",
    ":separatePatchReleases",
    ":prNotPending",
    ":automergeDigest",
    ":automergePatch",
    ":automergeLinters",
    ":automergeTypes",
    ":automergeTesters",
    "group:recommended",
    "helpers:disableTypesNodeMajor",
    "docker:pinDigests"
  ],
  "encrypted": {
    "npmToken": ""
  },
  "packageRules": [
    {
      "depTypeList": [
        "devDependencies"
      ],
      "prPriority": -1,
      "extends": [
        ":semanticCommitTypeAll(chore)"
      ]
    },
    {
      "groupName": "AWS SDK",
      "packageNames": [
        "aws-sdk"
      ],
      "schedule": [
        "after 9pm on sunday"
      ]
    },
    {
      "groupName": "Trade Platform",
      "packagePatterns": "^@trade-platform/",
      "schedule": [
        "after 6am every weekday",
        "before 9pm every weekday"
      ]
    }
  ],
  "hostRules": [
    {
      "hostType": "docker",
      "hostName": "153538148884.dkr.ecr.us-east-2.amazonaws.com",
      "encrypted": {
        "username": "",
        "password": ""
      }
    },
    {
      "hostType": "docker",
      "hostName": "quay.io",
      "encrypted": {
        "username": "",
        "password": ""
      }
    }
  ],
  "dependencyDashboard": true,
  "major": {
    "dependencyDashboardApproval": true
  },
  "minor": {
    "semanticCommitType": "feat",
    "bumpVersion": "minor"
  },
  "patch": {
    "semanticCommitType": "fix",
    "bumpVersion": "patch"
  },
  "prConcurrentLimit": 5,
  "timezone": "America/New_York",
  "stabilityDays": 3,
  "includeForks": true
}
